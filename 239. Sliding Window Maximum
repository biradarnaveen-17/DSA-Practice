class Solution {
    public int[] maxSlidingWindow(int[] nums, int k) {
        int n=nums.length;
        int[] c=new int[n-k+1];
        int a=0;

        for(int i=0;i<=n-k;i++)
        {
            int b=Integer.MIN_VALUE;
            for(int j=0+i;j<i+k;j++)
            {
                b=Math.max(nums[j],b);
            }
            c[a++]=b;
            
        }
        return c;
    }
}

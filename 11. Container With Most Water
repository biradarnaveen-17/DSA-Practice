class Solution {
    public int maxArea(int[] height) {
        int a=0;
        for(int i=0;i<height.length-1;i++)
        {
            for(int j=i+1;j<height.length;j++)
            {
                if(a<(Math.min(height[i], height[j])) * (j - i))
                {
                    a=(Math.min(height[i], height[j])) * (j - i);
                }

            }
        }
        return a;
    }
}
